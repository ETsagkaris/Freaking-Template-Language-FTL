  "steps_spec": {
    "steps": {
      "civil_info": {
        "display": {
          "default": {
            "el": {
              "title": "Στοιχεία πολίτη",
              "captionLeft": "Άντληση στοιχείων ταυτότητας από δημοσίους υπαλλήλους",
              "serviceName": "Ψηφιακό αντίγραφο δελτίου ταυτότητας",
              "serviceURL": "https://id-copy.services.gov.gr/"
            }
          }
        },
        "validator": "dilosi.applications.gov_present_id.claims.common.validate_citizen_info_input",
        "action-order": "update",
        "fieldset-order": "citizen_info citizen_application_info",
        "actions": {
          "update": {
            "display": {
              "default": {
                "el": {
                  "component": "document-update",
                  "label": "Συνέχεια",
                  "primary": true
                }
              }
            }
          }
        },
        "fieldsets": {
          "citizen_info": {
            "display": {
              "default": {
                "el": {
                  "title": "Στοιχεία πολίτη",
                  "component": "form"
                }
              },
              "pdf": {
                "el": {
                  "component": "hidden"
                }
              }
            },
            "field-order": "citizen_firstname citizen_surname citizen_fathername citizen_birthyear citizen_document_number"
          },
          "citizen_application_info": {
            "display": {
              "default": {
                "el": {
                  "title": "Στοιχεία αιτήματος πολίτη",
                  "component": "form"
                }
              },
              "pdf": {
                "el": {
                  "component": "hidden"
                }
              }
            },
            "field-order": "mitos_id mitos_onoma_diadikasias citizen_case_id"
          }
        },
        "fields": {
          "citizen_firstname": {
            "user-input-mode": "required",
            "datatype": "string",
            "display": {
              "default": {
                "el": {
                  "title": "Όνομα",
                  "hint": "Να συμπληρώνονται τα 3 πρώτα γράμματα του ονόματος",
                  "component": "string"
                }
              }
            }
          },
          "citizen_surname": {
            "user-input-mode": "required",
            "datatype": "string",
            "display": {
              "default": {
                "el": {
                  "title": "Επώνυμο",
                  "hint": "Να συμπληρώνονται τα 3 πρώτα γράμματα του επωνύμου",
                  "component": "string"
                }
              }
            }
          },
          "citizen_fathername": {
            "user-input-mode": "optional",
            "datatype": "string",
            "display": {
              "default": {
                "el": {
                  "title": "Όνομα Πατέρα",
                  "hint": "Να συμπληρώνονται τα 3 πρώτα γράμματα του πατρωνύμου. Εάν είναι \"ΑΝΕΥ ΠΑΤΡΟΣ\" αφήστε κενό το πεδίο ή συμπληρώστε με παύλα  \"-\".",
                  "component": "string"
                }
              }
            }
          },
          "citizen_birthyear": {
            "user-input-mode": "required",
            "datatype": "int",
            "display": {
              "default": {
                "el": {
                  "title": "Έτος γέννησης",
                  "component": "int"
                }
              }
            }
          },
          "citizen_document_number": {
            "user-input-mode": "required",
            "datatype": "string",
            "display": {
              "default": {
                "el": {
                  "title": "Αριθμός Δελτίου Ταυτότητας",
                  "hint": "Να συμπληρώνεται ο ΑΔΤ με κεφαλαίους ελληνικούς χαρακτήρες και τα ψηφία, χωρίς κενά ή παύλες",
                  "component": "string"
                }
              }
            }
          },
          "mitos_id": {
            "user-input-mode": "optional",
            "datatype": "string",
            "display": {
              "default": {
                "el": {
                  "title": "6ψήφιος κωδικός διαδικασίας MITOΣ",
                  "hint": "Να συμπληρώνεται ο εξαψήφιος κωδικός της διαδικασίας (του ΜΙΤΟΣ) για την οποία ζητείται το αντίγραφο ταυτότητας",
                  "component": "string"
                }
              }
            }
          },
          "mitos_onoma_diadikasias": {
            "user-input-mode": "optional",
            "datatype": "text",
            "display": {
              "default": {
                "el": {
                  "title": "Όνομα διαδικασίας",
                  "hint": "Εφόσον δεν βρεθεί διαδικασία στο ΜΙΤΟΣ παρακαλούμε να αναγράψετε το όνομα της διαδικασίας για την οποία ζητείται το αντίγραφο ταυτότητας.",
                  "component": "text"
                }
              }
            }
          },
          "citizen_case_id": {
            "user-input-mode": "required",
            "datatype": "string",
            "display": {
              "default": {
                "el": {
                  "title": "Αριθμός αιτήματος πολίτη",
                  "hint": "Καταγράψτε τον αριθμό πρωτοκόλλου και την ημερομηνία της αίτησης του πολίτη για την οποία απαιτείται η έκδοση του αντιγράφου ταυτότητας",
                  "component": "string"
                }
              }
            }
          }
        },
